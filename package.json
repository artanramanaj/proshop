  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "dotenv": "^17.0.1",
    "nodemon": "^3.1.10"
  }
}
